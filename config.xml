<?xml version='1.0' encoding='utf-8'?>
<widget id="industries.invent.prosign" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Prosign</name>
    <description>
        An open source morse code communicator.
    </description>
    <author email="prosign@invent.industries" href="http://prosign.ml">
        Invent Industies
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-vibration" spec="~2.1.0" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="~1.6.0" />
    <engine name="android" spec="~5.1.1" />
    <plugin name="cordova-plugin-uniquedeviceid" spec="~1.3.2" />
</widget>
